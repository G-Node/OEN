<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.pizza.com/ontologies/pizza.owl#"
     xml:base="http://www.pizza.com/ontologies/pizza.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.pizza.com/ontologies/pizza.owl">
        <rdfs:comment>A pizza ontology that describes various pizzas based on their toppings</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasBase">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isBaseOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin"/>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasIngredient">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasTopping">
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isToppingOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isBaseOf">
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasIngredient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#isToppingOf">
        <rdfs:range rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:domain rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
        <rdfs:subPropertyOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.pizza.com/ontologies/pizza.owl#hasCalorificContentValue">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pizza.com/ontologies/pizza.owl#AmericanHotPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#AmericanHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#AmericanaPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#AnchovyTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SeaFoodTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CaperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetableTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CheeseTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#CheesyPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Country -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Country">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#France"/>
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#England"/>
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#America"/>
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Italy"/>
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Germany"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#DeepPanBase -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#FourCheesePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:onClass rdf:resource="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">4</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#HamTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="&xsd;integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="&xsd;integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Hot -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Hot">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#InterestingPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">3</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#LowCaloriePizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="&xsd;integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="&xsd;integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#MargheritaPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#MeatTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Medium -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Medium">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#Mild -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#Mild">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#MozzarellaTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#CheeseTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasCountryOfOrigin"/>
                <owl:hasValue rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Italy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#ParmezanTopping"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#MushroomTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#MushroomTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetableTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.com/ontologies/pizza.owl#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#NamedPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#NamedPizza">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#NonVegetarianPizza -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#NonVegetarianPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.pizza.com/ontologies/pizza.owl#Pizza"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetarianPizza"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetarianPizza"/>
    </owl:Class>
    


    <!-- http://www.pizza.com/ontologies/pizza.owl#OliveTopping -->

    <owl:Class rdf:about="http://www.pizza.com/ontologies/pizza.owl#OliveTopping">
        <rdfs:subClassOf rdf:resource="http://www.pizza.com/ontologies/pizza.owl#VegetableTopping"/>
